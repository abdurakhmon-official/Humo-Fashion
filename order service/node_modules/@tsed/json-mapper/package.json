  "dependencies": {
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@tsed/barrels": "8.17.2",
    "@tsed/core": "8.17.2",
    "@tsed/schema": "8.17.2",
    "@tsed/typescript": "8.17.2",
    "eslint": "9.12.0",
    "typescript": "5.4.5",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@tsed/core": ">=8.0.0",
    "@tsed/schema": ">=8.0.0"
  },
  "peerDependenciesMeta": {
